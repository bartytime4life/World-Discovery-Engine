# FILE: .env.example
# -------------------------------------------------------------------------------------------------
# Copy to .env and fill before running tools/ or CLI.

# Kaggle API (for CLI uploads/reads if you add submit helpers)
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_key

# Remote sensing providers (add what you actually use)
PLANET_API_KEY=your_planet_key
SENTINELHUB_CLIENT_ID=your_client_id
SENTINELHUB_CLIENT_SECRET=your_client_secret
EARTHDATA_USERNAME=
EARTHDATA_PASSWORD=

# Maps/tiles
MAPBOX_TOKEN=

# WDE runtime
WDE_DATA_ROOT=./data
WDE_ARTIFACTS_ROOT=./artifacts